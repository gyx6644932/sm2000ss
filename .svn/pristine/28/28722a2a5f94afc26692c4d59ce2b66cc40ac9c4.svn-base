<div class="row scrollY" >
    <div class="col-md-12">
        <h3>用户{{userTitle}}</h3>
        <div class="boxH"></div>
        <form class="form-horizontal formBiao">
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-3 col-sm-offset-1 control-label">用户名称: </label>
                <div class="col-sm-5">
                    <input type="text" class="form-control" ng-model="formData.userName">
                </div>
            </div>
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-3 col-sm-offset-1 control-label">密码: </label>
                <div class="col-sm-5">
                    <input type="text" class="form-control" ng-model="formData.password">
                </div>
            </div>
             <div class="form-group" style="text-align: center">
                  <label for="inputEmail3" class="col-sm-3 col-sm-offset-1 control-label">角色分配: </label>
                  <div
                    class="col-sm-5"
                    isteven-multi-select 
                    input-model="iformData" 
                    output-model="oformData"
                    button-label="roleName roleDesc"        
                    item-label="roleName roleDesc"        
                    tick-property="ticked"                
                    >
                </div>  
             </div> 
            <div class="form-group">
                <div class="col-sm-offset-5 col-sm-4">
                    <input type="button" class="btn btn-warning btn-md" value="返回" ng-click="backArea()">
                    <input type="button" class="btn btn-warning btn-md" value="确认" ng-disabled="disabledFlag" ng-click="userManageSub(formData)">
                </div>
            </div>
        </form>
    </div>
</div>

