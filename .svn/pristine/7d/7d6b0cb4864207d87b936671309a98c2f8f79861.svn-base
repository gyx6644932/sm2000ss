  background-color: #16A977
}

.logo{  top: 32%;
  position: absolute;
  color: #fff;
  font-size: 25px;
  font-family: cursive;
  font-weight: bold;
}
.oMenu{position: absolute;right:0px;max-width: 920px;text-align: center;overflow: hidden;bottom: -1px;right: 0px;}
.oMenu ul {
display: -webkit-box;
display: -webkit-flex;
display: -ms-flexbox;
display: flex;
-webkit-justify-content: space-around;
-ms-flex-pack: distribute;
justify-content: space-around;
list-style: none;
padding: 0;
margin: 0;
}
.oMenu ul li{
    width: 96px;
    height: 35px;
    line-height: 35px;
}
.oMenu ul li > a {
  display: block;
  /*padding: 10px 20px;*/
  text-decoration: none;
  border-radius: 3px;
  color: antiquewhite;
  text-shadow: 0 1px 0 black;
  -webkit-transition: all ease .25s;
  transition: all ease .25s;
  text-shadow: -10px 0px 25px red,10px 0px 25px red;
}
.oMenu ul li > a:hover {
  background-color: rgba(0,0,0,0.7);
  color: #fff;
  font-weight: bold;
  text-shadow: 4px 4px 75px #FFF;}
.activeOneMenu{
  background-color: rgba(0,0,0,0.7);
  color: #fff;
  font-weight: bold;
  text-shadow: 4px 4px 25px #FFF;}
}

.tree{width: 80%;margin: 12px auto;}
.treeLeft{height: 100%;width: 100%;overflow: hidden;}
.treeDom{height: 90%;overflow: auto;}

.bys{display: inline-block;border-radius: 5px;background: #EFEAD0;border: 1px solid #79784D;text-align: center;height: 26px;line-height: 26px;}
.userOut{position: absolute;top:3px;right:0px;margin-right: 20px;color:white;}
.userOut p{display: inline;vertical-align: bottom;}
.twoMenu{width: 100%;height: 100%;margin: auto;background-color: whitesmoke}
.tuopu img{width:20px;position: initial;}
.sbNode img{width: 60px;}
.tree-light li{
  margin-top: 10px;
