.view{line-height: 96%;margin: auto;height: 96%;width: 98%;margin-top: 20px;border-radius: 8px;}
.content-s{height: 90%;padding: 0 10px;overflow: auto;overflow-x: hidden;}
.login{position: relative;left: 50%;top: 50%;background: #ccc;height: 200px;width: 300px;margin-left: -150px;margin-top: 300px;}
.user,.pas{width: 100px;height: 22px;}
