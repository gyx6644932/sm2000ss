<div class="left" >
	<div class="leftViet view">
		<div ng-include="'template/twoMenu.html'" style="height: 100%;width:100%;"></div>
	</div>
</div>
<div class="right">
	<div class="rightView view" ui-view>
		<div class="content-s">
			<div class="row">
				<div class="col-md-12 querySeach">
					<h3>性能表单</h3>
					<div class="boxH"></div>
					<br></div>
			</div>
			<div class="col-md-12 querySeach">
				<div>
					<span >设备:</span>
					<select ng-model="devID" style="width:123px;"  class="form-inline input-sm" ng-options="m.ip group by m.parentInfo.name for m in deviceInfo">
						<option value="">请选择</option>
					</select>
					<label class="control-label">类型:</label>
					<select ng-model="performanceFrameM" ng-change="performanceChange()"  class="form-inline input-sm" style="height: 26px;">
						<option value="0">mtie</option>
						<option value="1">tdev</option>
						<option value="2">freq</option>
					</select>
					<input type="button" class="btn btn-warning btn-md" value="搜索" ng-click="seach(performanceFrameM)"></div>
				<br></div>
			<div class="row tableTs" >
				<div class="col-md-12">
					<table ng-if="performanceFrameM == 0" class="table table-bordered table-hover table-condensed">
						<thead>
							<tr >
								<th class="t-middle" style="width:8%"></th>
								<th class="t-middle" style="width:8%">1</th>
								<th class="t-middle" style="width:8%">4</th>
								<th class="t-middle" style="width:8%">10</th>
								<th class="t-middle" style="width:8%">40</th>
								<th class="t-middle" style="width:8%">100</th>
								<th class="t-middle" style="width:8%">300</th>
								<th class="t-middle" style="width:8%">900</th>
								<th class="t-middle" style="width:8%">1800</th>
								<th class="t-middle" style="width:8%">3600</th>
								<th class="t-middle" style="width:8%">7200</th>
								<th class="t-middle" style="width:8%">14400</th>
								<th class="t-middle" style="width:8%">28800</th>
								<th class="t-middle" style="width:8%">86400</th>
							</tr>
						</thead>
						<tbody>
							<tr ng-repeat="item in performanceList">
								<td ng-if="$index < 4 ">slot1-{{$index+1}}</td>
								<td ng-if="$index >= 4 ">slot2-{{$index-3}}</td>
								<td>{{item.x[0]}}</td>
								<td>{{item.x[1]}}</td>
								<td>{{item.x[2]}}</td>
								<td>{{item.x[3]}}</td>
								<td>{{item.x[4]}}</td>
								<td>{{item.x[5]}}</td>
								<td>{{item.x[6]}}</td>
								<td>{{item.x[7]}}</td>
								<td>{{item.x[8]}}</td>
								<td>{{item.x[9]}}</td>
								<td>{{item.x[10]}}</td>
								<td>{{item.x[11]}}</td>
								<td>{{item.x[12]}}</td>
							</tr>
							<tr ng-if="performanceList.length == 0">
								<td colspan="2" class="textCenter">
									<span>暂无数据！</span>
								</td>
							</tr>
						</tbody>
					</table>

					<table ng-if="performanceFrameM == 1 || performanceFrameM == 2" class="table table-bordered table-hover table-condensed">
						<thead>
							<tr >
								<th class="t-middle" style="width:7%"></th>
								<th class="t-middle" style="width:7%">1</th>
								<th class="t-middle" style="width:7%">2</th>
								<th class="t-middle" style="width:7%">4</th>
								<th class="t-middle" style="width:7%">8</th>
								<th class="t-middle" style="width:7%">16</th>
								<th class="t-middle" style="width:7%">32</th>
								<th class="t-middle" style="width:7%">64</th>
								<th class="t-middle" style="width:7%">128</th>
								<th class="t-middle" style="width:7%">256</th>
								<th class="t-middle" style="width:7%">512</th>
								<th class="t-middle" style="width:7%">1024</th>
							</tr>
						</thead>
						<tbody>
							<tr ng-repeat="item in performanceList">
								<td ng-if="$index < 4 ">slot1-{{$index+1}}</td>
								<td ng-if="$index >= 4 ">slot2-{{$index-3}}</td>
								<td>{{item.x[0]}}</td>
								<td>{{item.x[1]}}</td>
								<td>{{item.x[2]}}</td>
								<td>{{item.x[3]}}</td>
								<td>{{item.x[4]}}</td>
								<td>{{item.x[5]}}</td>
								<td>{{item.x[6]}}</td>
								<td>{{item.x[7]}}</td>
								<td>{{item.x[8]}}</td>
								<td>{{item.x[9]}}</td>
								<td>{{item.x[10]}}</td>
							</tr>
							<tr ng-if="performanceList.length == 0">
								<td colspan="2" class="textCenter">
									<span>暂无数据！</span>
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>